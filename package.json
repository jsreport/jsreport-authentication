{
  "name": "jsreport-authentication",
  "version": "1.0.3",
  "description": "jsreport extension adding user authentication",
  "scripts": {
    "test": "mocha test --timeout 10000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authentication",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authentication.git"
  },
  "keywords": [
    "jsreport",
    "authentication"
  ],
  "standard": {
    "ignore": [
      "main.js",
      "public/**"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "body-parser": "1.15.1",
    "client-sessions": "0.7.0",
    "ejs": "2.4.2",
    "in-publish": "2.0.0",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "password-hash": "1.2.2",
    "q": "1.4.1",
    "shortid": "2.2.6",
    "string": "3.3.1",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "1.0.4",
    "jsreport-express": "1.0.2",
    "jsreport-studio-dev": "1.0.0",
    "mocha": "2.5.3",
    "should": "9.0.1",
    "standard": "7.1.2",
    "supertest": "1.2.0"
  },
  "files": [
    "lib", "public", "studio", "index.js", "jsreport.config.js"
  ]
}
