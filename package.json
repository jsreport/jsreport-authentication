{
  "name": "jsreport-authentication",
  "version": "1.2.0",
  "description": "jsreport extension adding user authentication",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha \"./test/*.spec.js\" --timeout 10000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authentication",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authentication.git"
  },
  "keywords": [
    "jsreport",
    "authentication"
  ],
  "standard": {
    "ignore": [
      "main.js",
      "public/**"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "client-sessions": "0.7.0",
    "ejs": "2.5.6",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "password-hash": "1.2.2",
    "request": "2.81.0",
    "shortid": "2.2.8",
    "string": "3.3.3",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.2.1",
    "eslint": "3.19.0",
    "eslint-config-standard-jsx": "3.3.0",
    "eslint-plugin-babel": "4.1.1",
    "express": "4.15.2",
    "in-publish": "2.0.0",
    "jsreport-core": "1.2.2",
    "jsreport-express": "1.2.2",
    "jsreport-studio-dev": "1.0.2",
    "mocha": "3.2.0",
    "should": "11.2.1",
    "standard": "9.0.2",
    "supertest": "3.0.0"
  },
  "files": [
    "lib",
    "public",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
