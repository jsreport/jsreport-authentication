{
  "name": "jsreport-authentication",
  "version": "2.0.0",
  "description": "jsreport extension adding user authentication",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha \"./test/*.spec.js\" --timeout 10000 --exit && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authentication",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authentication.git"
  },
  "keywords": [
    "jsreport",
    "authentication"
  ],
  "standard": {
    "ignore": [
      "main.js",
      "public/**"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "client-sessions": "0.8.0",
    "ejs": "2.5.7",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "password-hash": "1.2.2",
    "request": "2.83.0",
    "shortid": "2.2.8"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "express": "4.16.2",
    "in-publish": "2.0.0",
    "jsreport-core": "2.0.0",
    "jsreport-express": "2.0.0",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "2.0.0",
    "lodash.clonedeep": "4.5.0",
    "mocha": "5.0.0",
    "should": "13.2.1",
    "standard": "10.0.3",
    "supertest": "3.0.0"
  },
  "files": [
    "lib",
    "public",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}
